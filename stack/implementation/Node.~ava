public class Node<ContentType>{
  Node NT;
  ContentType Inhalt;
  public Node(ContentType pContent){
     this.Inhalt = pContent;
    }
  public ContentType getInhalt(){
      return this.Inhalt;
    }
  
  public Node getNT(){
     return this.NT;
    }    
  
  public void setNT(Node pNT){
    this.NT = pNT;
    }
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  }